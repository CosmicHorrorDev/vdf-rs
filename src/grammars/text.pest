WHITESPACE = _{ " " | "\t" | "\r" | "\n" }

vdf = _{ SOI ~ pairs ~ EOI }

pairs = { pair* }

pair = { string ~ value }

value = _{ string | obj }

obj = { "{" ~ pair* ~ "}" }

string = ${ "\"" ~ inner ~ "\"" }
inner = @{ char* }
char = {
    !("\"" | "\\")  ~ ANY
    | "\\" ~ ("\"" | "\\" | "n" | "r" | "t")
}
