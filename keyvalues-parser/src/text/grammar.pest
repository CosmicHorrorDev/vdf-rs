WHITESPACE = _{ " " | "\t" | "\r" | "\n" }

vdf = _{ SOI ~ pair ~ EOI }

pairs = { pair* }

pair = { key ~ value }

key = _{ string }

value = _{ string | obj }

obj = { "{" ~ pairs ~ "}" }

string = ${ "\"" ~ inner ~ "\"" }
inner = @{ char* }
char = {
    !("\"" | "\\")  ~ ANY
    | "\\" ~ ("\"" | "\\" | "n" | "r" | "t")
}
